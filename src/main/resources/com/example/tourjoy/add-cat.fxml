<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CatController">
   <children>
      <TextField fx:id="nameField" layoutX="35.0" layoutY="53.0" />
      <Button fx:id="nameAdd" layoutX="49.0" layoutY="151.0" mnemonicParsing="false" onAction="#addCategories" text="add" />
      <Button fx:id="nameDelete" layoutX="75.0" layoutY="226.0" mnemonicParsing="false" onAction="#deleteCategories" text="delete" />
      <Button fx:id="nameUpdate" layoutX="133.0" layoutY="151.0" mnemonicParsing="false" onAction="#updateCategories" text="update" />
      <TableView fx:id="View" layoutX="231.0" layoutY="39.0" prefHeight="259.0" prefWidth="346.0">
        <columns>
          <TableColumn fx:id="idView" prefWidth="75.0" text="id" />
          <TableColumn fx:id="nameView" prefWidth="75.0" text="name" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
