<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="377.0" prefWidth="738.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CatController">
   <children>
      <TextField fx:id="nameField" layoutX="24.0" layoutY="123.0" prefHeight="26.0" prefWidth="181.0" />
      <Button fx:id="nameAdd" layoutX="24.0" layoutY="199.0" mnemonicParsing="false" onAction="#addCategories" prefHeight="26.0" prefWidth="54.0" text="Add" />
      <Button fx:id="nameDelete" layoutX="151.0" layoutY="200.0" mnemonicParsing="false" onAction="#deleteCategories" prefHeight="26.0" prefWidth="54.0" text="Delete" />
      <Button fx:id="nameUpdate" layoutX="87.0" layoutY="200.0" mnemonicParsing="false" onAction="#updateCategories" text="Update" />
      <TableView fx:id="View" layoutX="231.0" layoutY="39.0" prefHeight="272.0" prefWidth="444.0">
        <columns>
          <TableColumn fx:id="idView" prefWidth="75.0" text="Id" />
          <TableColumn fx:id="nameView" prefWidth="75.0" text="Name" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
